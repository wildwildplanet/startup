    "scheme": "startup-pitch-challenge",
    "jsEngine": "hermes",
    "name": "Startup Pitch Challenge",
    "slug": "startup-pitch-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "jsEngine": "hermes",
      "supportsTablet": true,
      "bundleIdentifier": "com.startuppitchchallenge",
      "infoPlist": {
        "SKAdNetworkItems": [
